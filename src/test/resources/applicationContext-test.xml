<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <!-- bean definitions here -->
    <bean id="languageFileReader" class="language.LanguageFileReader" />

    <bean id="dictionary" class="language.Dictionary">
        <constructor-arg ref="languageFileReader"/>
    </bean>

    <bean id="language" class="language.Language">
        <constructor-arg ref="languageFileReader"/>
        <constructor-arg ref="dictionary"/>
    </bean>
</beans>